template(name='tokenCollectionReport')
  h1 Token Collection Report
  .download
    a(href='#') Download csv

  table.table.table-striped
    thead
      tr
        th Beverage Station
        each collectionDates
          th {{date}}
        th Total
    tbody
      each reportTable
        tr
          td {{name}}
          each locationTotals
            td {{total}}
